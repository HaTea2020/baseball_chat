<div class="side_top">
  <div class="my_name">
  <%if user_signed_in?%>
    <%=link_to current_user do%>
      <%=current_user.nickname%>
    <%end%>
  <%else%>
    <p>仮ユーザー</p>
  <%end%>
  </div>
</div>
<div class="side_under">
<%if user_signed_in?%>
  <% current_user.rooms.each do |room|%>
    <div class="room">
      <div class="side_room_name">
        <%=link_to room.name, room_messages_path(room)%>
      </div>
    </div>
  <%end%>
<%end%>
</div>